#!/usr/bin/python3
import sys
import time

def factorize(number):
    factors = []
    if number % 2 == 0:
        factors.append(int(2))
        factors.append(int(number / 2))
    elif number % 3 == 0:
        factors.append(int(3))
        factors.append(int(number / 3))
    else:
        for i in range(5, int((number)**0.5) + 1):
            if number % i == 0:
                factors.append(int(number / i))
                factors.append(int(i))
                break
    return (factors)


def main(input_file):
    try:
        with open(input_file, 'r') as file:
            lines = file.read().splitlines()
            for line in lines:
                number = int(line)
                factors = factorize(number)
                if len(factors) >= 2:
                    p = factors[1]
                    q = factors[0]
                    print(f"{number}={p}*{q}")
    except FileNotFoundError:
        print(f"Excepton: File '{input_file}' not found.")
    except (TypeError, ValueError):
        print("Exception: Invalid input. File should only contain valid natural numbers greater than 1.")
    except Exception as e:
        print("Exception: {}".format(e), file=sys.stderr)


if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./factors <file>")
        sys.exit(1)
    start_time = time.time()
    input_file = sys.argv[1]
    main(input_file)
    elapsed_time = time.time() - start_time
    if elapsed_time >= 5:
        sys.exit(1)